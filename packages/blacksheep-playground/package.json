{
  "name": "@blacksheep/playground",
  "version": "0.0.1",
  "description": "A game engine in full typescript",
  "main": "dist/index.js",
  "scripts": {
    "compile": "shx rm -rf ./dist && ./node_modules/.bin/tsc",
    "test": "npm run compile && npm run mocha",
    "mocha": "mocha --require ts-node/register test/**/*.test.ts",
    "copy-client": "shx mkdir ./dist/client && shx cp ./node_modules/@blacksheep/playground-client/dist/blacksheep-playground-client.js* ./dist/client",
    "build": "npm run compile && shx cp -rf ./src/screen ./dist && npm run copy-client"
  },
  "dependencies": {
    "express": "^4.17.1",
    "config": "^3.3.3",
    "uuid": "^8.3.2",
    "socket.io": "^3.1.2",
    "@hermes/composition": "^2.0.7",
    "@blacksheep/playground-client": "^0.0.0",
    "node_modules-path": "^2.0.5"
  },
  "devDependencies": {
    "typescript": "^4.1.3",
    "electron": "^10.1.1",
    "mocha": "^8.3.1",
    "chai": "^4.3.3",
    "chai-as-promised": "^7.1.1",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.1.1",
    "axios": "^0.21.1",
    "@types/node": "^14.14.22",
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.2.1",
    "@types/chai": "^4.2.15",
    "@types/chai-as-promised": "^7.1.3",
    "@types/axios": "^0.14.0",
    "@types/uuid": "^8.3.0",
    "@types/socket.io": "^2.1.13",
    "shx": "^0.3.3"
  },
  "keywords": [
    "game",
    "engine"
  ],
  "author": "gabrieldausque.pro@gmail.com",
  "license": "ISC"
}
